		"prettier-plugin-tailwindcss": "^0.2.7",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"tailwind-merge": "^1.12.0",
		"tailwindcss": "^3.3.1",
		"typescript": "^5.0.3",
		"vite": "^4.2.1",
		"vitest": "^0.29.8"
	},
	"type": "module",
	"lint-staged": {
		"*.{js,ts,svelte,html,cjs,json}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	}
